package org.tukorea.center;

import org.tukorea.club.*;

public class StudentCenter {
	public static void main(String[] args) {
		ProgClub club = new ProgClub();
		club.addMember(new ProgClubMember("철수", "1201", "소프트웨어", "파이썬", "남성"));
		club.addMember(new ProgClubMember("영숙", "1202", "소프트웨어", "파이썬", "남성"));
		club.addMember(new ProgClubMember("미희", "2401", "소프트웨어", "파이썬", "남성"));
		club.addMember(new ProgClubMember("길동", "2402", "소프트웨어", "파이썬", "남성"));
	}
}